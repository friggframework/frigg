{
  "name": "@friggframework/ui-svelte",
  "version": "0.1.0",
  "description": "Svelte integration for Frigg UI Core",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./src/index.js",
      "default": "./dist/index.js"
    },
    "./stores": {
      "types": "./dist/stores/index.d.ts",
      "svelte": "./src/stores/index.js",
      "default": "./dist/stores/index.js"
    },
    "./components": {
      "types": "./dist/components/index.d.ts",
      "svelte": "./src/components/index.js",
      "default": "./dist/components/index.js"
    },
    "./actions": {
      "types": "./dist/actions/index.d.ts",
      "svelte": "./src/actions/index.js",
      "default": "./dist/actions/index.js"
    }
  },
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "build:analyze": "ANALYZE=true vite build",
    "preview": "vite preview",
    "package": "svelte-kit sync && svelte-package && publint",
    "prepublishOnly": "npm run package",
    "test": "npm run test:unit && npm run test:integration",
    "test:unit": "vitest",
    "test:integration": "playwright test",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "eslint .",
    "format": "prettier --write ."
  },
  "files": [
    "dist",
    "src",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "peerDependencies": {
    "@friggframework/ui-core": "^1.0.0",
    "svelte": "^4.0.0 || ^5.0.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.28.1",
    "@sveltejs/adapter-auto": "^3.0.0",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/package": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "@testing-library/svelte": "^4.0.5",
    "@types/eslint": "^8.56.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.35.1",
    "jsdom": "^24.0.0",
    "prettier": "^3.1.1",
    "prettier-plugin-svelte": "^3.1.2",
    "publint": "^0.1.9",
    "svelte": "^4.2.7",
    "svelte-check": "^3.6.0",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "vite": "^5.0.11",
    "vitest": "^1.2.0",
    "rollup-plugin-visualizer": "^5.12.0",
    "terser": "^5.31.0"
  },
  "svelte": "./src/index.js",
  "keywords": [
    "frigg",
    "ui",
    "svelte",
    "sveltekit",
    "integration",
    "framework"
  ],
  "author": "Frigg Framework Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/friggframework/frigg.git",
    "directory": "packages/ui/svelte"
  },
  "bugs": {
    "url": "https://github.com/friggframework/frigg/issues"
  },
  "homepage": "https://github.com/friggframework/frigg/tree/main/packages/ui-svelte#readme"
}